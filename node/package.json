{
  "name": "@io-pipeline/grpc-stubs",
  "version": "1.0.8",
  "description": "TypeScript Connect-ES stubs for Pipeline Engine services, generated from Protocol Buffers",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./health": "./dist/grpc/health/v1/health_pb.js",
    "./registration": "./dist/registration/platform_registration_pb.js",
    "./shell": "./dist/frontend/shell_service_pb.js",
    "./upload": "./dist/repository/filesystem/upload/upload_service_pb.js",
    "./account": "./dist/repository/account/account_service_pb.js",
    "./connector": "./dist/module/connectors/connector_intake_service_pb.js",
    "./pipedoc": "./dist/repository/pipedoc/pipedoc_service_pb.js",
    "./repository/filesystem": "./dist/repository/filesystem/filesystem_service_pb.js",
    "./mapping": "./dist/mapping-service/mapping_service_pb.js",
    "./module": "./dist/module/module_service_pb.js",
    "./core": {
      "import": "./dist/core/pipeline_core_types_pb.js",
      "types": "./dist/core/pipeline_core_types_pb.d.ts"
    }
  },
  "files": [
    "dist/**/*.js",
    "dist/**/*.d.ts",
    "dist/**/*.d.ts.map",
    "dist/**/*.js.map"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "copy-src": "cp src/*.ts dist/",
    "generate": "buf generate ../grpc-stubs/src/main/proto && npm run copy-src && tsc",
    "build": "npm run clean && npm run generate",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://git.rokkon.com/io-pipeline/grpc.git",
    "directory": "node"
  },
  "bugs": {
    "url": "https://git.rokkon.com/io-pipeline/grpc/issues"
  },
  "homepage": "https://git.rokkon.com/io-pipeline/grpc#readme",
  "license": "Apache-2.0",
  "keywords": [
    "grpc",
    "protobuf",
    "typescript",
    "connect-es",
    "pipeline",
    "stubs"
  ],
  "author": "Pipeline Team",
  "publishConfig": {
    "registry": "https://git.rokkon.com/api/packages/io-pipeline/npm"
  },
  "devDependencies": {
    "@bufbuild/buf": "^1.56.0",
    "@bufbuild/protoc-gen-es": "latest",
    "typescript": "^5.9.2"
  },
  "dependencies": {
    "@bufbuild/protobuf": "latest",
    "@connectrpc/connect": "latest",
    "@connectrpc/connect-web": "latest"
  },
  "engines": {
    "node": ">=22.0.0 <23",
    "pnpm": ">=10 <11"
  }
}
